{
	"http": {
		"port": 8800,
		"auth": {
			"realm": "ddns-proxy",
			"file": "./tests/auth.htpasswd"
		},
		"ssl": {
			"use_ssl": false,
			"cert_path": "cert.pem",
			"pkey_path": "pkey.pem"		
		}
	},

	"ddns-proxy": {
		"headers": {
			"Authentication": "Digest {{token}}"
		},
		"create": {
			"url": "http://localhost:1000/new"
		},
		"update": {
			"url": "http://localhost:1000/update"
		}
	}
}
